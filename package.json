{
  "name": "react-boss",
  "version": "1.1.7",
  "description": "A React component generation library",
  "repository": {
    "type": "git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Torinit Technologies",
  "license": "MIT",
  "main": "bin/react-make.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && babel bin/react-make.js --retain-lines -s -d dist"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "keywords": [
    "react",
    "test",
    "generator",
    "cli",
    "jest",
    "container",
    "component"
  ],
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "coveralls": "3.0.3",
    "json": "^9.0.6",
    "lodash": "^4.17.21",
    "minimist": "1.2.3",
    "node-plop": "0.18.0",
    "plop": "^2.5.2",
    "pre-commit": "1.2.2",
    "prettier": "1.17.0",
    "prettier-config-standard": "^1.0.1",
    "shelljs": "^0.8.3"
  },
  "@babel/cli": "7.4.3",
  "bin": {
    "react-boss": "bin/react-make.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
